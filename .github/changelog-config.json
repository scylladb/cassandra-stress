{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ§° Maintenance",
      "labels": ["chore", "dependencies"]
    },
    {
      "title": "## ğŸ“ Documentation",
      "labels": ["documentation"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "#{{CHANGELOG}}#{{UNCATEGORIZED}}",
  "pr_template": "- #{{TITLE}} by @#{{AUTHOR}} in ##{{NUMBER}}",
  "empty_template": "- No changes",
  "category_template": "### #{{TITLE}}\n\n#{{BODY}}\n",
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\(.+\\))?!?:",
      "target": "$1"
    }
  ],
  "transformers": [
    {
      "pattern": "^feat:",
      "target": "feature"
    },
    {
      "pattern": "^fix:",
      "target": "bug"
    },
    {
      "pattern": "^docs:",
      "target": "documentation"
    },
    {
      "pattern": "^chore:",
      "target": "chore"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "base_branches": [
    "master",
    "main"
  ]
}
